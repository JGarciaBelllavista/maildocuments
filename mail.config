FACTURAS_SQL_QUERY3=SELECT INV1.ItemCode, INV1.Dscription, INV1.Quantity, PriceO = INV1.Price, INV1.VatPrcnt, LineTotal = CASE WHEN(INV1.Currency = 'EUR') THEN INV1.LineTotal WHEN (INV1.Currency != 'EUR' and OINV.CurSource = 'L') THEN INV1.LineTotal ELSE INV1.TotalFrgn END, Price1 = ITM1.Price, INV1.DiscPrcnt, INV1.PriceBefDi, INV1.treeType FROM INV1 left join ITM1 ON INV1.ItemCode = ITM1.ItemCode and ITM1.PriceList = (select 'listnum' = CASE OCRD.Country when 'GB' then '56' when 'ES' then '52' else '55' end from OCRD left join OINV on OINV.CardCode = OCRD.CardCode where OINV.DocEntry = '&STR_RPL&') left join OINV ON OINV.DocEntry = INV1.DocEntry WHERE INV1.DocEntry = '&STR_RPL&' |SELECT INV1.ItemCode, INV1.Dscription, INV1.Quantity, PriceO = INV1.Price, INV1.VatPrcnt, LineTotal = CASE WHEN(INV1.Currency = 'EUR') THEN INV1.LineTotal WHEN (INV1.Currency != 'EUR' and OINV.CurSource = 'L') THEN INV1.LineTotal ELSE INV1.TotalFrgn END, Price1 = ITM1.Price, INV1.DiscPrcnt, INV1.PriceBefDi, INV1.treeType FROM INV1 left join ITM1 ON INV1.ItemCode = ITM1.ItemCode and ITM1.PriceList = (select 'listnum' = CASE OCRD.Country when 'GB' then '12' when 'ES' then '5' else '6' end from OCRD left join OINV on OINV.CardCode = OCRD.CardCode where OINV.DocEntry = '&STR_RPL&') left join OINV ON OINV.DocEntry = INV1.DocEntry WHERE INV1.DocEntry = '&STR_RPL&' 
