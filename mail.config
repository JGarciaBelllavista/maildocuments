PEDIDOS_SQL_QUERY=SELECT TOP 100  COUNT(*) OVER () as Total_Rows, ORDR.DocNum, DocDate = FORMAT(ORDR.DocDate,'dd/MM/yyyy'), ORDR.CardCode, DocTotal = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DocTotal ELSE ORDR.DocTotalFC END, VatSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.VatSum ELSE ORDR.VatSumFC END, DiscSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DiscSum ELSE ORDR.DiscSumFC END, ORDR.DocCur, TotalExpns = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.TotalExpns ELSE ORDR.TotalExpFC END, ORDR.DiscPrcnt, ORDR.Address, ORDR.Address2, ORDR.LicTradNum, OCRD.CardName, OCRD.CardFName, ORDR.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ORDR.Comments, ORDR.DocEntry, OADM.CompnyName,OVTG.Rate, OVTG.EquVatPr, ORDR.EquVatSum , BankCode = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.' or ORDR.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.' or ORDR.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END,OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum, ORDR.U_SEITipoPedido, ORDR.U_SEI_ENVIOMAIL FROM ORDR left join OCRD ON OCRD.CardCode = ORDR.CardCode left join OSLP ON OSLP.SlpCode = ORDR.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORDR.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct, OADM WHERE ((ORDR.U_SEI_ENVIOMAIL is null and ORDR.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (ORDR.U_SEI_ENVIOMAIL = '3' and ORDR.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, -1, CONVERT (date, GETDATE())))) and ORDR.CANCELED = 'N' and ORDR.CardCode not like 'CW%' and ORDR.CardCode not like 'C00494%' and ORDR.CardCode not like 'C04740%' and ORDR.CardCode not like 'C05732%' and ORDR.CardCode not like 'CA0001%' and ORDR.CardCode not like 'CP04288' and ORDR.CardCode not like 'C01297' and ORDR.CardCode not like 'C05280' and ORDR.CardCode not like 'C05709' and ORDR.CardCode not like 'P00015' and ORDR.CardCode not like 'CR03751%' and ORDR.CardCode not like 'C06129' and ORDR.CardCode not like 'C06111' and ORDR.CardCode not like 'C06163' and ORDR.CardCode not like 'C09999' order by ORDR.CreateDate desc;|SELECT TOP 100 COUNT(*) OVER () as Total_Rows, ORDR.DocNum, DocDate = FORMAT(ORDR.DocDate,'dd/MM/yyyy'), ORDR.CardCode, DocTotal = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DocTotal ELSE ORDR.DocTotalFC END, VatSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.VatSum ELSE ORDR.VatSumFC END, DiscSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DiscSum ELSE ORDR.DiscSumFC END, ORDR.DocCur, TotalExpns = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.TotalExpns ELSE ORDR.TotalExpFC END, ORDR.DiscPrcnt, ORDR.Address, ORDR.Address2, ORDR.LicTradNum, OCRD.CardName, OCRD.CardFName, ORDR.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ORDR.Comments, ORDR.DocEntry, OADM.CompnyName,OVTG.Rate, OVTG.EquVatPr, ORDR.EquVatSum ,BankCode = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE OCRD.HouseBank+' '+OCRD.HousBnkBrn+' '+OCRD.HousCtlKey+' '+OCRD.HousBnkAct END, BankName = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.') THEN T1.BankName ELSE T0.BankName END, OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum, ORDR.U_SEITipoPedido, ORDR.U_SEI_ENVIOMAIL FROM ORDR left join OCRD ON OCRD.CardCode = ORDR.CardCode left join OSLP ON OSLP.SlpCode = ORDR.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORDR.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp, OADM WHERE ((ORDR.U_SEI_ENVIOMAIL is null and ORDR.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (ORDR.U_SEI_ENVIOMAIL = '3' and ORDR.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, -1, CONVERT (date, GETDATE())))) and ORDR.CANCELED = 'N' and ORDR.CardCode not like 'CP04288' and ORDR.CardCode not like 'C09999' order by ORDR.CreateDate desc;
