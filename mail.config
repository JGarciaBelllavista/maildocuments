PEDIDOSUSA_SQL_QUERY0=SELECT 1 FROM (VALUES(1)) t(a);
PEDIDOSUSA_SQL_QUERY=SELECT TOP 100 COUNT(*) OVER () as Total_Rows, ORDR.DocNum, DocDate = FORMAT(ORDR.DocDate,'dd/MM/yyyy'), ORDR.CardCode, DocTotal = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DocTotal ELSE ORDR.DocTotalFC END, VatSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.VatSum ELSE ORDR.VatSumFC END, DiscSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DiscSum ELSE ORDR.DiscSumFC END, ORDR.DocCur, TotalExpns = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.TotalExpns ELSE ORDR.TotalExpFC END, ORDR.DiscPrcnt, ORDR.Address, ORDR.Address2, ORDR.LicTradNum, OCRD.CardName, OCRD.CardFName, ORDR.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ORDR.Comments, ORDR.DocEntry, OADM.CompnyName,OVTG.Rate, OVTG.EquVatPr, ORDR.EquVatSum ,BankCode = CASE WHEN(T0.BankName IS NULL or T0.BankCode = '') THEN OCRD.BankCode+' '+OCRD.DflBranch+' '+OCRD.BankCtlKey+' '+OCRD.DflAccount ELSE OCRD.HouseBank+' '+OCRD.HousBnkBrn+' '+OCRD.HousCtlKey+' '+OCRD.HousBnkAct END, BankName = CASE WHEN(T0.BankName IS NULL or T0.BankCode = '') THEN T1.BankName ELSE T0.BankName END, OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum, ORDR.U_SEITipoPedido, ORDR.U_SEI_ENVIOMAIL FROM ORDR left join OCRD ON OCRD.CardCode = ORDR.CardCode left join OSLP ON OSLP.SlpCode = ORDR.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORDR.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp, OADM WHERE (ORDR.U_SEI_ENVIOMAIL is null or ORDR.U_SEI_ENVIOMAIL = '3') and ORDR.CANCELED = 'N'  and ORDR.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE())) and  (ORDR.CardCode LIKE 'C05732%'or  ORDR.CardCode LIKE 'CR03751%'or  ORDR.CardCode = 'C05709');
PEDIDOSUSA_SQL_QUERY2=SELECT DueDate = FORMAT(RDR6.DueDate,'dd/MM/yyyy'), LineTotal = CASE WHEN(RDR6.InsTotalFC = 0) THEN RDR6.InsTotal ELSE RDR6.InsTotalFC END FROM RDR6 WHERE RDR6.DocEntry = '&STR_RPL&';
PEDIDOSUSA_SQL_QUERY3=SELECT RDR1.ItemCode, RDR1.Dscription, RDR1.Quantity, PriceO = RDR1.Price, LineTotal = CASE WHEN(RDR1.Currency = 'EUR') THEN RDR1.LineTotal WHEN (RDR1.Currency != 'EUR' and ORDR.CurSource = 'L') THEN RDR1.LineTotal ELSE RDR1.TotalFrgn END, OITM.U_seiFoto3, Price1 = '-1', RDR1.DiscPrcnt, RDR1.PriceBefDi, RDR1.treeType FROM RDR1 left join OITM ON RDR1.ItemCode = OITM.ItemCode left join ORDR ON ORDR.DocEntry = RDR1.DocEntry WHERE RDR1.DocEntry = '&STR_RPL&' 
PEDIDOSUSA_SQL_QUERY3_ORDER1=ORDER BY RDR1.LineNum ASC;
PEDIDOSUSA_SQL_QUERY3_ORDER2=ORDER BY RDR1.Itemcode ASC;
