FACTURAS_SQL_QUERY=SELECT TOP 100 COUNT(*) OVER () as Total_Rows, OINV.DocNum, DocDate = FORMAT(OINV.DocDate,'dd/MM/yyyy'), OINV.CardCode, DocTotal = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.DocTotal ELSE OINV.DocTotalFC END, VatSum = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.VatSum ELSE OINV.VatSumFC END, DiscSum = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.DiscSum ELSE OINV.DiscSumFC END, OINV.DocCur, TotalExpns = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.TotalExpns ELSE OINV.TotalExpFC END, OINV.DiscPrcnt, OINV.Address, OINV.Address2, OINV.LicTradNum, OCRD.CardName, OCRD.CardFName, OINV.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, OINV.Comments, OINV.DocEntry, OADM.CompnyName,Rate = CASE WHEN(OVTG.Rate is null and OCRD.VatStatus = 'Y') THEN 21 ELSE OVTG.Rate END, OVTG.EquVatPr, OINV.EquVatSum ,BankCode = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END,OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum,Street = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.Street IS NOT NULL AND DSC1.ZipCode IS NOT NULL) THEN DSC1.Street+' '+DSC1.ZipCode END,City   = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.City IS NOT NULL AND DSC1.County IS NOT NULL) THEN DSC1.City+' ('+DSC1.County+')' END, U_SEI_Tracking, U_SEI_Transportista, OCRD.MailZipCod, OCRD.U_SEIcyc FROM OINV left join OCRD ON OCRD.CardCode = OINV.CardCode left join OSLP ON OSLP.SlpCode = OINV.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = OINV.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct, OADM WHERE ((OINV.U_SEI_ENVIOMAILF is null and OINV.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (OINV.U_SEI_ENVIOMAILF = '3' and OINV.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, -1, CONVERT (date, GETDATE())))) and OINV.CardCode not like 'CR%' and OINV.CardCode not like 'C00494%' and OINV.CardCode not like 'C04740%' and OINV.CardCode not like 'C05732%' and OINV.CardCode not like 'CA0001%' and OINV.CardCode not like 'CP04288' and OINV.CardCode not like 'C01297' and OINV.CardCode not like 'C00746' and OINV.CardCode not like 'C09999';|SELECT TOP 100 COUNT(*) OVER () as Total_Rows, OINV.DocNum, DocDate = FORMAT(OINV.DocDate,'dd/MM/yyyy'), OINV.CardCode, DocTotal = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.DocTotal ELSE OINV.DocTotalFC END, VatSum = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.VatSum ELSE OINV.VatSumFC END, DiscSum = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.DiscSum ELSE OINV.DiscSumFC END, OINV.DocCur, TotalExpns = CASE WHEN(OINV.DocCur = 'EUR') THEN OINV.TotalExpns ELSE OINV.TotalExpFC END, OINV.DiscPrcnt, OINV.Address, OINV.Address2, OINV.LicTradNum, OCRD.CardName, OCRD.CardFName, OINV.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, OINV.Comments, OINV.DocEntry, OADM.CompnyName,Rate = CASE WHEN(OVTG.Rate is null and OCRD.VatStatus = 'Y') THEN 21 ELSE OVTG.Rate END, OVTG.EquVatPr, OINV.EquVatSum ,BankCode = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END, OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum ,Street = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.Street IS NOT NULL AND DSC1.ZipCode IS NOT NULL) THEN DSC1.Street+' '+DSC1.ZipCode END,City   = CASE WHEN(OINV.PeyMethod = 'GIRO DOMICIL.' or OINV.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.City IS NOT NULL AND DSC1.County IS NOT NULL) THEN DSC1.City+' ('+DSC1.County+')' END, OCRD.MailZipCod, OCRD.U_SEIcyc FROM OINV left join OCRD ON OCRD.CardCode = OINV.CardCode left join OSLP ON OSLP.SlpCode = OINV.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = OINV.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct, OADM WHERE ((OINV.U_SEI_ENVIOMAILF is null and OINV.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (OINV.U_SEI_ENVIOMAILF = '3' and OINV.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, -1, CONVERT (date, GETDATE()))))  and OINV.CardCode not like 'C09999';
