ABONOS_SQL_QUERY=SELECT TOP 100  COUNT(*) OVER () as Total_Rows, ORIN.DocNum,DocDate = FORMAT(ORIN.DocDate,'dd/MM/yyyy'),ORIN.CardCode,DocTotal = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.DocTotal ELSE ORIN.DocTotalFC END, VatSum = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.VatSum ELSE ORIN.VatSumFC END, DiscSum = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.DiscSum ELSE ORIN.DiscSumFC END,ORIN.DocCur, TotalExpns = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.TotalExpns ELSE ORIN.TotalExpFC END, ORIN.DiscPrcnt,ORIN.Address,ORIN.Address2,ORIN.LicTradNum,OCRD.CardName,OCRD.CardFName,ORIN.PeyMethod,OCTG.PymntGroup, OSLP.SlpName,ORIN.Comments,ORIN.DocEntry,OADM.CompnyName,Rate = CASE WHEN(OVTG.Rate is null and OCRD.VatStatus = 'Y') THEN 21 ELSE OVTG.Rate END, OVTG.EquVatPr,ORIN.EquVatSum, BankCode = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END, OCRD.Country,OCRD.E_Mail,OSLP.U_SEIREPRE,T2.Name,OSLP.SlpCode,OADM.TaxIdNum, Street = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.Street IS NOT NULL AND DSC1.ZipCode IS NOT NULL) THEN DSC1.Street+' '+DSC1.ZipCode END, City   = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.City IS NOT NULL AND DSC1.County IS NOT NULL) THEN DSC1.City+' ('+DSC1.County+')' END,  U_SEI_Tracking,U_SEI_Transportista,ORIN.DocType,UFD1.Descr FROM ORIN left join OCRD ON OCRD.CardCode = ORIN.CardCode left join OSLP ON OSLP.SlpCode = ORIN.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORIN.GroupNum  left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct left join UFD1 on UFD1.FldValue = ORIN.U_SEITipoAbono,OADM  WHERE ((ORIN.U_SEI_ENVIOMAILF is null and ORIN.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (ORIN.U_SEI_ENVIOMAILF = '3' and ORIN.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE())))) and (UFD1.TableID='ORIN' or UFD1.TableID is null) and (UFD1.FieldID='24' or UFD1.FieldID is null) and ORIN.CardCode not like 'CR%' and ORIN.CardCode not like 'CW%' and ORIN.CardCode not like 'CM%' and ORIN.CardCode not like 'C00494%' and ORIN.CardCode not like 'C04740%' and ORIN.CardCode not like 'C05732%' and ORIN.CardCode not like 'CA0001%' and ORIN.CardCode not like 'CZ%' and ORIN.CardCode not like 'CB%' and ORIN.CardCode not in ('CP04288' ,'C01297', 'C00746' , 'C09999',  'C20000','C07739', 'C07571', 'CT00002','C07787','C05474', 'C05473', 'C00016', 'C07553', 'C05491', 'C05644','C05515','C05495', 'C05524', 'C05544', 'C05546','C05548', 'C05540', 'C05545', 'CR07589');|SELECT TOP 100 COUNT(*) OVER () as Total_Rows, ORIN.DocNum,DocDate = FORMAT(ORIN.DocDate,'dd/MM/yyyy'), ORIN.CardCode, DocTotal = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.DocTotal ELSE ORIN.DocTotalFC END, VatSum = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.VatSum ELSE ORIN.VatSumFC END, DiscSum = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.DiscSum ELSE ORIN.DiscSumFC END, ORIN.DocCur, TotalExpns = CASE WHEN(ORIN.DocCur = 'EUR') THEN ORIN.TotalExpns ELSE ORIN.TotalExpFC END, ORIN.DiscPrcnt, ORIN.Address, ORIN.Address2, ORIN.LicTradNum, OCRD.CardName, OCRD.CardFName, ORIN.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ORIN.Comments, ORIN.DocEntry, OADM.CompnyName,Rate = CASE WHEN(OVTG.Rate is null and OCRD.VatStatus = 'Y') THEN 21 ELSE OVTG.Rate END, OVTG.EquVatPr, ORIN.EquVatSum ,BankCode = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END, OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum ,Street = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.Street IS NOT NULL AND DSC1.ZipCode IS NOT NULL) THEN DSC1.Street+' '+DSC1.ZipCode END,City   = CASE WHEN(ORIN.PeyMethod = 'GIRO DOMICIL.' or ORIN.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.City IS NOT NULL AND DSC1.County IS NOT NULL) THEN DSC1.City+' ('+DSC1.County+')' END, ORIN.DocType, UFD1.Descr FROM ORIN left join OCRD ON OCRD.CardCode = ORIN.CardCode left join OSLP ON OSLP.SlpCode = ORIN.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORIN.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct left join UFD1 on UFD1.FldValue = ORIN.U_SEITipoAbono, OADM WHERE ((ORIN.U_SEI_ENVIOMAILF is null and ORIN.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (ORIN.U_SEI_ENVIOMAILF = '3' and ORIN.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE())))) and UFD1.TableID='ORIN' and UFD1.FieldID = '14' and ORIN.CardCode not like 'C09999' and ORIN.CardCode not like 'CT00002' and ORIN.CardCode not like 'C00494%' and ORIN.CardCode not like 'CZ%' and ORIN.CardCode not like 'CM%';
ABONOS_SQL_QUERY2=SELECT DueDate = FORMAT(RIN6.DueDate,'dd/MM/yyyy'), LineTotal = CASE WHEN(RIN6.InsTotalFC is null or RIN6.InsTotalFC = 0) THEN RIN6.InsTotal ELSE RIN6.InsTotalFC END FROM RIN6 WHERE RIN6.DocEntry = '&STR_RPL&';|SELECT DueDate = FORMAT(INV6.DueDate,'dd/MM/yyyy'), LineTotal = CASE WHEN(INV6.InsTotalFC is null or INV6.InsTotalFC = 0) THEN INV6.InsTotal ELSE INV6.InsTotalFC END FROM INV6 WHERE INV6.DocEntry = '&STR_RPL&';
ABONOS_SQL_QUERY3=SELECT RIN1.ItemCode,RIN1.Dscription,RIN1.Quantity,PriceO = RIN1.Price, RIN1.VatPrcnt, LineTotal = CASE WHEN(RIN1.Currency = 'EUR') THEN RIN1.LineTotal WHEN (RIN1.Currency != 'EUR' and ORIN.CurSource = 'L') THEN RIN1.LineTotal ELSE RIN1.TotalFrgn END, Price1 = ITM1.Price,RIN1.DiscPrcnt,RIN1.PriceBefDi,RIN1.treeType,RIN1.AcctCode FROM RIN1  left join ITM1 ON RIN1.ItemCode = ITM1.ItemCode and ITM1.PriceList =  (select 'listnum' = CASE OCRD.Country when 'GB' then '56' when 'ES' then '52' else '55' end from OCRD left join ORIN on ORIN.CardCode = OCRD.CardCode where ORIN.DocEntry = '&STR_RPL&') left join ORIN ON ORIN.DocEntry = RIN1.DocEntry WHERE RIN1.DocEntry = '&STR_RPL&' |SELECT RIN1.ItemCode, RIN1.Dscription, RIN1.Quantity, PriceO = RIN1.Price, RIN1.VatPrcnt, LineTotal = CASE WHEN(RIN1.Currency = 'EUR') THEN RIN1.LineTotal WHEN (RIN1.Currency != 'EUR' and ORIN.CurSource = 'L') THEN RIN1.LineTotal ELSE RIN1.TotalFrgn END, Price1 = ITM1.Price, RIN1.DiscPrcnt, RIN1.PriceBefDi, RIN1.treeType, RIN1.AcctCode FROM RIN1 left join ITM1 ON RIN1.ItemCode = ITM1.ItemCode and ITM1.PriceList = (select 'listnum' = CASE OCRD.Country when 'GB' then '12' when 'ES' then '5' else '6' end from OCRD left join ORIN on ORIN.CardCode = OCRD.CardCode where ORIN.DocEntry = '&STR_RPL&') left join ORIN ON ORIN.DocEntry = RIN1.DocEntry WHERE RIN1.DocEntry = '&STR_RPL&' 
