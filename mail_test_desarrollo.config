PEDIDOS_SQL_QUERY=SELECT TOP 100  COUNT(*) OVER () as Total_Rows, ORDR.DocNum, DocDate = FORMAT(ORDR.DocDate,'dd/MM/yyyy'), ORDR.CardCode, DocTotal = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DocTotal ELSE ORDR.DocTotalFC END, VatSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.VatSum ELSE ORDR.VatSumFC END, DiscSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DiscSum ELSE ORDR.DiscSumFC END, ORDR.DocCur, TotalExpns = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.TotalExpns ELSE ORDR.TotalExpFC END, ORDR.DiscPrcnt, ORDR.Address, ORDR.Address2, ORDR.LicTradNum, OCRD.CardName, OCRD.CardFName, ORDR.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ORDR.Comments, ORDR.DocEntry, OADM.CompnyName,OVTG.Rate, OVTG.EquVatPr, ORDR.EquVatSum , BankCode = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.' or ORDR.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.' or ORDR.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END,OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum, ORDR.U_SEITipoPedido, ORDR.U_SEI_ENVIOMAIL,ORDR.U_SEI_DNPO FROM ORDR left join OCRD ON OCRD.CardCode = ORDR.CardCode left join OSLP ON OSLP.SlpCode = ORDR.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORDR.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct, OADM WHERE ORDR.DocNum = 21049868 AND ((ORDR.U_SEI_ENVIOMAIL is null and ORDR.CreateDate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE()))) or (ORDR.U_SEI_ENVIOMAIL = '1' and ORDR.CreateDate BETWEEN DATEADD(dd, -365, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE())))) and ORDR.CANCELED = 'N' and ORDR.CardCode not like 'CW%' and ORDR.CardCode not like 'C00494%' and ORDR.CardCode not like 'C04740%' and ORDR.CardCode not like 'C05732%' and ORDR.CardCode not like 'CA0001%' and ORDR.CardCode not like 'CR03751%' and ORDR.CardCode not like 'CZ%' and ORDR.CardCode not like 'CM%' and ORDR.CardCode not like 'CB%' and ORDR.CardCode not in ('CP04288','C01297','C05280', 'C05709','P00015','C06129','C06111' , 'C06163', 'C07571','C07739','C20000','C09999', 'C00746', 'CT00002', 'C06473','C07787','C05474','C05473', 'C00016', 'C07553', 'C05491', 'C05644','C05515','C05495', 'C05524', 'C05544', 'C05546','C05548', 'C05540', 'C05545', 'C03026', 'CR07589') order by ORDR.CreateDate desc;|SELECT TOP 100 COUNT(*) OVER () as Total_Rows, ORDR.DocNum, DocDate = FORMAT(ORDR.DocDate,'dd/MM/yyyy'), ORDR.CardCode, DocTotal = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DocTotal ELSE ORDR.DocTotalFC END, VatSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.VatSum ELSE ORDR.VatSumFC END, DiscSum = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.DiscSum ELSE ORDR.DiscSumFC END, ORDR.DocCur, TotalExpns = CASE WHEN(ORDR.DocCur = 'EUR') THEN ORDR.TotalExpns ELSE ORDR.TotalExpFC END, ORDR.DiscPrcnt, ORDR.Address, ORDR.Address2, ORDR.LicTradNum, OCRD.CardName, OCRD.CardFName, ORDR.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ORDR.Comments, ORDR.DocEntry, OADM.CompnyName,OVTG.Rate, OVTG.EquVatPr, ORDR.EquVatSum ,BankCode = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE OCRD.HouseBank+' '+OCRD.HousBnkBrn+' '+OCRD.HousCtlKey+' '+OCRD.HousBnkAct END, BankName = CASE WHEN(ORDR.PeyMethod = 'GIRO DOMICIL.') THEN T1.BankName ELSE T0.BankName END, OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum, ORDR.U_SEITipoPedido, ORDR.U_SEI_ENVIOMAIL,ORDR.U_SEI_DNPO FROM ORDR left join OCRD ON OCRD.CardCode = ORDR.CardCode left join OSLP ON OSLP.SlpCode = ORDR.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ORDR.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp, OADM WHERE ORDR.DocNum = 21049868 AND ORDR.CANCELED = 'N' and ORDR.CardCode not like 'CW%' and ORDR.CardCode not like 'CP04288' and ORDR.CardCode not like 'C09999'  and ORDR.CardCode not like 'CT00002' and ORDR.CardCode not like 'CZ%' and ORDR.CardCode not like 'C00494%' and ORDR.CardCode not like 'CM%' order by ORDR.CreateDate desc;
