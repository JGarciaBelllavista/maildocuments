FACTURASUSA_SQL_QUERY0=SELECT TOP 100 COUNT(*) OVER () as Total_Rows, baseref, Comments, DocDate = FORMAT(oinv.docdate,'MM/dd/yyyy') from oinv,inv1 where oinv.docentry = inv1.docentry  AND oinv.cardcode LIKE 'C05732%'  AND oinv.docdate BETWEEN DATEADD(dd, -10, CONVERT (date, GETDATE())) AND DATEADD(dd, 0, CONVERT (date, GETDATE())) group by baseref,Comments,oinv.docdate  order by baseref desc;
FACTURASUSA_SQL_QUERY=SELECT TOP 100 COUNT(*) OVER () as Total_Rows, ODLN.DocNum, DocDate = FORMAT(ODLN.DocDate,'MM/dd/yyyy'), ODLN.CardCode, DocTotal = CASE WHEN(ODLN.DocCur = 'EUR') THEN ODLN.DocTotal ELSE ODLN.DocTotalFC END, VatSum = CASE WHEN(ODLN.DocCur = 'EUR') THEN ODLN.VatSum ELSE ODLN.VatSumFC END, DiscSum = CASE WHEN(ODLN.DocCur = 'EUR') THEN ODLN.DiscSum ELSE ODLN.DiscSumFC END, ODLN.DocCur, TotalExpns = CASE WHEN(ODLN.DocCur = 'EUR') THEN ODLN.TotalExpns ELSE ODLN.TotalExpFC END, ODLN.DiscPrcnt, ODLN.Address, ODLN.Address2, ODLN.LicTradNum, OCRD.CardName, OCRD.CardFName, ODLN.PeyMethod, OCTG.PymntGroup, OSLP.SlpName, ODLN.Comments, ODLN.DocEntry, OADM.CompnyName,Rate = CASE WHEN(OVTG.Rate is null and OCRD.VatStatus = 'Y') THEN 21 ELSE OVTG.Rate END, OVTG.EquVatPr, ODLN.EquVatSum ,BankCode = CASE WHEN(ODLN.PeyMethod = 'GIRO DOMICIL.' or ODLN.PeyMethod = 'GIRO NO DOMICIL') THEN '******************'+RIGHT(OCRD.DflAccount,4) ELSE 'IBAN:  '+DSC1.IBAN+'  SWIFT:'+ODSC.SwiftNum END, BankName = CASE WHEN(ODLN.PeyMethod = 'GIRO DOMICIL.' or ODLN.PeyMethod = 'GIRO NO DOMICIL') THEN T1.BankName ELSE T0.BankName END,OCRD.Country, OCRD.E_Mail, OSLP.U_SEIREPRE, T2.Name, OSLP.SlpCode, OADM.TaxIdNum,Street = CASE WHEN(ODLN.PeyMethod = 'GIRO DOMICIL.' or ODLN.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.Street IS NOT NULL AND DSC1.ZipCode IS NOT NULL) THEN DSC1.Street+' '+DSC1.ZipCode END,City   = CASE WHEN(ODLN.PeyMethod = 'GIRO DOMICIL.' or ODLN.PeyMethod = 'GIRO NO DOMICIL') THEN NULL WHEN (DSC1.City IS NOT NULL AND DSC1.County IS NOT NULL) THEN DSC1.City+' ('+DSC1.County+')' END, U_SEI_Tracking, U_SEI_Transportista, OCRD.MailZipCod FROM ODLN left join OCRD ON OCRD.CardCode = ODLN.CardCode left join OSLP ON OSLP.SlpCode = ODLN.SlpCode left join OVTG ON OVTG.Code = OCRD.ECVatGroup left join OCTG ON OCTG.GroupNum = ODLN.GroupNum left join ODSC T0 ON OCRD.HouseBank = T0.BankCode left join ODSC T1 ON OCRD.BankCode = T1.BankCode left join [@SEITEMPORADA] T2 on T2.Code = U_SEITemp left join ODSC on T0.BankCode = ODSC.BankCode left join DSC1 on DSC1.BankCode = T0.BankCode and DSC1.Account = OCRD.HousBnkAct, OADM WHERE (ODLN.U_SEI_NumFactUSA = '0' or ODLN.U_SEI_NumFactUSA = '' or ODLN.U_SEI_NumFactUSA is null ) AND ODLN.DocNum = '&STR_RPL&';
FACTURASUSA_SQL_QUERY2=SELECT DueDate = FORMAT(INV6.DueDate,'MM/dd/yyyy'), LineTotal = CASE WHEN(INV6.InsTotalFC is null or INV6.InsTotalFC = 0) THEN INV6.InsTotal ELSE INV6.InsTotalFC END FROM INV6 WHERE INV6.DocEntry = '&STR_RPL&';
FACTURASUSA_SQL_QUERY3=SELECT DLN1.ItemCode, DLN1.Dscription, DLN1.Quantity, PriceO = DLN1.Price, DLN1.VatPrcnt, LineTotal = CASE WHEN(DLN1.Currency = 'EUR') THEN DLN1.LineTotal WHEN (DLN1.Currency != 'EUR' and ODLN.CurSource = 'L') THEN DLN1.LineTotal ELSE DLN1.TotalFrgn END, Price1 = ITM1.Price, DLN1.DiscPrcnt, DLN1.PriceBefDi, DLN1.treeType FROM DLN1 left join ITM1 ON DLN1.ItemCode = ITM1.ItemCode and ITM1.PriceList = '65' left join ODLN ON ODLN.DocEntry = DLN1.DocEntry WHERE DLN1.lineStatus = 'C'  AND  DLN1.DocEntry = '&STR_RPL&' 
FACTURASUSA_SQL_QUERY3_ORDER1= ORDER BY DLN1.Itemcode ASC;
FACTURASUSA_SQL_QUERY3_ORDER2= ORDER BY DLN1.Itemcode ASC;
